<% include ./partials/_head %>
<body>
<% include ./partials/_header %>

  <div class="row">

  <div class="col-md-2"></div>
  <div class="col-md-8">
    <ul class="list-group">
      <% for(url in urls){ %>
      <li class="list-group-item content-transparency">
        <div class="row decor">
        <div class="col-md-8 invert">
          <%=url%> &xrarr; <%= urls[url]%>
          </div>
        <div class="col-md-2">
          <form method="POST" action="/urls/<%=url%>/delete" style="display:inline;">
            <button class="btn btn-danger">delete</button>
          </form>
        </div>
        <div class="col-md-2">
        <a href="/urls/<%=url%>">
        <button class="btn btn-warning">edit</button>
          </a>
          </div>
        </div>
      </li>
      <%}%>
    </ul>
  </div>
  <div class="col-md-2"></div>
</div>
